  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "npm run start:desktop",
    "start:desktop": "tsc -p tsconfig.main.json --outDir dist/desktop && cross-env GAME_PLATFORM=desktop NODE_ENV=development webpack --watch",
    "start:desktop:prod": "cross-env NODE_ENV=production npm run start:desktop",
    "start:web": "npm run serve",
    "start:web:prod": "cross-env NODE_ENV=production npm run serve",
    "serve": "cross-env GAME_PLATFORM=browser webpack-dev-server --public --host 0.0.0.0 --content-base ./dist/web --port 2333 --inline",
    "build:electron:main": "tsc -p tsconfig.main.json --outDir dist/desktop && cross-env GAME_PLATFORM=desktop",
    "build": "webpack --display-error-details && npm run build:electron:main",
    "build:web": "mkdir -p ./dist/web ./dist/web && cross-env GAME_PLATFORM=browser NODE_ENV=production webpack",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "game": "electron ./dist/desktop/main.js --serve $*",
    "electron:serve": "npm run build:electron:main && electron ./dist/desktop/main.js --serve",
    "electron:test": "electron ./dist/desktop/main.js --serve",
    "electron:dev": "npm run build && electron ./dist/desktop/main.js",
    "electron:prod": "npm run build:prod && electron ./dist/desktop/main.js",
    "electron:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "electron:windows": "npm run build && node package.js --platform=win32 --arch=ia32",
    "electron:mac": "npm run build:prod && node package.js --platform=darwin --arch=x64 --asar"
  },
